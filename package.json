{
  "name": "a",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.config.js",
  "scripts": {
    "dev": "npm-run-all -p dev:*",
    "dev:server": "nodemon -w server --exec npm-run-all --serial  dev:server:*",
    "dev:server:build": " cross-env NODE_ENV=production  webpack",
    "dev:server:exec": "node dist/server.js",
    "dev:client": "npm-run-all -p dev:client:*",
    "dev:client:front": "cd front && npm run serve",
    "dev:client:back": "cd back && npm start",
    "prod": "npm-run-all -p prod:**",
    "prod:server:build": "cross-env NODE_ENV=production  webpack",
    "prod:client:front": "cd front && npm run build",
    "prod:client:back": "cd back && npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.3.0"
  }
}